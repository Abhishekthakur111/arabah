
<div class="app-content content">
    <div class="content-overlay"></div>
    <div class="header-navbar-shadow"></div>
    <div class="content-wrapper container-xxl p-0">
        <div class="content-header row">
        </div>
        <div class="content-body">
            <!-- users list start -->
            <section class="app-user-list">
                <!-- users filter start -->
                <!-- users filter end -->
                <!-- list section start -->
                <div class="card">
                    <div class="card-datatable table-responsive pt-0">
                        <table id="myTable" class="user-list-table table">
                            <thead class="table-light">
                                <tr>
                                    <th>Sr.no</th>
                                    <th>Image</th>
                                    <th>Name</th>
                                    <th>Email</th>
                                    <th>Phone.no</th>
                                    <th>Status</th>
                                    <th>Action</th>
                                </tr>
                            </thead>
                            <tbody>
                                <% data.forEach((item, i) => { %>
                                <tr>
                                    <td><%= i + 1 %></td>
                                    <td>
                                        <img src="../<%= item.image %>" alt="<%= item.image %>" class="rounded-circle" style="height: 50px; width: 50px;">
                                    </td>
                                    <td><%= item.name %></td>
                                    <td><%= item.email %></td>
                                    <td><%= item.phone_number %></td>
                                    <td>
                                        <% if (item.status == 0) { %>
                                            <a data-id="<%= item.id %>" data-id1="1" class="btn btn-info divClass">Inactive</a>
                                        <% } else { %>
                                            <a data-id="<%= item.id %>" data-id1="0" class="btn btn-warning divClass">Active</a>
                                        <% } %>
                                    </td>
                                    <td>
                                        <a href="/getuser/<%= item.id %>" class="btn btn-success"><i class="me-80" data-feather="eye"></i></a>
                                        <!-- <a href="/edit/<%= item.id %>" class="btn btn-primary"><i class="me-80" data-feather="edit"></i></a> -->
                                        <a onclick="deleteUser('<%= item.id %>')" class="btn btn-danger"><i class="me-80" data-feather="trash"></i></a>
                                    </td>
                                </tr>
                                <% }) %>
                            </tbody>
                        </table>
                    </div>
                    <!-- Modal to add new user starts-->
                    <!-- Modal to add new user Ends-->
                </div>
                <!-- list section end -->
            </section>
            <!-- users list ends -->
        </div>
    </div>
</div>